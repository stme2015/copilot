{
  "name": "codegen-pilot",
  "dependencies": {"axios": "^0.24.0"},
  "displayName": "CodeCompletionExtension",
  "description": "Provides code completion snippets and suggestions based on your prompt",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.completeCode",
      "title": "Complete Code"
    }],
    "keybindings": [
      {
        "command": "extension.completeCode",
        "key": "ctrl+alt+c",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
